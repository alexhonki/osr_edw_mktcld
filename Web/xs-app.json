{
	"authenticationMethod": "route",
	"routes": [{
		"source": "/(.*)",
		"destination": "Services_url",
		"csrfProtection": true,
		"scope": "$XSAPPNAME.QUERY",
		"authenticationType": "xsuaa"
	}, {
		"source": "^/(.*)$",
		"localDir": "resources",
		"scope": ["$XSAPPNAME.QUERY"],
		"replace": {
			"pathSuffixes": ["index.html"],
			"vars": ["ui5liburl"]
		}
	}]
}